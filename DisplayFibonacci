//Displaying Fibonacci series to the nth position
#include<iostream>
using namespace std;
int main()
{
    int last=0;
    int prev=1;
    int current;
    int n;
    cout<<"enter the position:";
    cin>>n;
    cout<<0<<" "<<1;
    for(int i=3;i<=n;i++)
    {
        current=last+prev;
        cout<<" "<<current<<" ";
        last=prev;
        prev=current;
    }
    return 0;
}
